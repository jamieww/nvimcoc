{
    "vim.useSystemClipboard": true,
    "editor.fontSize": 18,
    "vim.hlsearch": true,
    "vim.incsearch": true,
    "vim.leader": "<space>",
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.normal": ["#303030", "#939393"],
    // "vim.statusBarColors.insert": ["#BF616A", "#7C160D"],
    "vim.statusBarColors.insert": ["#BF616A", "#E8E8D5"],
    "vim.statusBarColors.visual": ["#265E84", "#98DAF8"],
    "vim.statusBarColors.visualline": "#B48EAD",
    "vim.statusBarColors.visualblock": "#A3BE8C",
    "vim.statusBarColors.replace": "#D08770",
    "vim.statusBarColors.commandlineinprogress": ["#5C6445", "#E8E8D5"],
    "vim.statusBarColors.searchinprogressmode": ["#5C6445", "#E8E8D5"],
    "vim.autoSwitchInputMethod.enable": true,
    "vim.autoSwitchInputMethod.defaultIM": "com.apple.keylayout.ABC",
    "vim.autoSwitchInputMethod.obtainIMCmd": "/usr/local/bin/im-select",
    "vim.autoSwitchInputMethod.switchIMCmd": "/usr/local/bin/im-select {im}",
    "vim.insertModeKeyBindings": [
        {
            "before":["j","k"],
            "after":["<Esc>"]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before":["`"],
            "after":["<Esc>"]
        },
        {
            "before":["leader","g","r"],
            "commands":[
                "vscode-postgres.runQuery"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [">"],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": ["<"],
            "commands": [
                "editor.action.outdentLines"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["leader",":"],
            "commands": [
                "workbench.action.showCommands",
            ]
        },
        {
            "before":["c"],
            "after":[
                "g","c"
            ]
        },
        {
            "before":["C"],
            "after":[
                "g","C"
            ]
        },
        {
            "before":["leader","g","c"],
            "commands":[
                "git.commitAll"
            ]
        },
        {
            "before":["leader","g","p"],
            "commands":[
                "git.push"
            ]
        },
        {
            "before":["leader","g","l"],
            "commands":[
                "git.pull"
            ]
        },
        {
            "before":["leader","r"],
            "commands":[
                "workbench.action.debug.start"
            ]
        },
        {
            "before":["leader","g","r"],
            "commands":[
                "vscode-postgres.runQuery"
            ]
        },
        {
            "before":["leader","g","s"],
            "commands":[
                "vscode-postgres.selectConnection"
            ]
        },
        {
            "before":["leader","g","h"],
            "commands":[
                "workbench.action.quickOpen"
            ]
        },
        {
            "before":["leader","g","d"],
            "commands":[
                "editor.action.peekDefinition"
            ]
        },
        {
            "before":["leader","g","i"],
            "commands":[
                "editor.action.goToImplementation"
            ]
        },
        {
            "before":["leader","m","m"],
            "commands":[
                "bookmarks.toggle"
            ]
        },
        {
            "before":["leader","m","t"],
            "commands":[
                "bookmarks.toggleLabeled"
            ]
        },
        {
            "before":["m","m"],
            "commands":[
                "bookmarks.jumpToNext"
            ]
        },
        {
            "before":["m","k"],
            "commands":[
                "bookmarks.jumpToPrevious"
            ]
        },
        {
            "before":["m","l"],
            "commands":[
                "bookmarks.list"
            ]
        },
        {
            "before":["leader","m","a"],
            "commands":[
                "bookmarks.listFromAllFiles"
            ]
        },
        {
            "before":["leader","m","s"],
            "commands":[
                "workbench.view.extension.bookmarks"
            ]
        },
        {
            "before":["leader","m","c"],
            "commands":[
                "bookmarks.clear"
            ]
        },
        {
            "before":["leader","m","c","c"],
            "commands":[
                "bookmarks.clearFromAllFiles"
            ]
        },
        {
            "before":["leader","j"],
            "commands":[
                ":tabn"
            ]
        },
        {
            "before":["leader","k"],
            "commands":[
                ":tabp"
            ]
        },
        {
            "before":["leader","h"],
            "commands":[
                ":tabfirst"
            ]
        },
        {
            "before":["leader","l"],
            "commands":[
                ":tablast"
            ]
        }
    ],
    "editor.lineNumbers": "relative",
    "markdown-preview-enhanced.enableExtendedTableSyntax": true,
    "markdown-preview-enhanced.previewTheme": "vue.css",
    "go.formatTool": "goimports",
    "editor.minimap.enabled": false,
    "breadcrumbs.enabled": false,
    "workbench.activityBar.visible": false,
    "go.useLanguageServer": true,
    "go.languageServerExperimentalFeatures": {
        "format": true,
        "autoComplete": true,
        "rename": true,
        "goToDefinition": true,
        "hover": true,
        "signatureHelp": true,
        "goToTypeDefinition": true,
        "goToImplementation": true,
        "documentSymbols": true,
        "workspaceSymbols": true,
        "findReferences": true,
        "diagnostics": true,
        "documentLink": true
    },
    "editor.fontFamily": "Inconsolata for Powerline",
    "editor.wordWrapColumn": 160,
    "editor.lineHeight": 20,
    "workbench.colorCustomizations": {
        "statusBar.background": "#303030",
        "statusBar.noFolderBackground": "#303030",
        "statusBar.debuggingBackground": "#303030",
        "statusBar.foreground": "#939393"
    },
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Material Theme Darker High Contrast",
    "sync.gist": "9d660dcf26e470f411639727bdd30cef",
    "git.autofetch": true,
    "editor.wordWrap": "on",
    "vscode-postgres.defaultConnection": "postgresql",
    "diffEditor.renderSideBySide": true,
    "bookmarks.saveBookmarksInProject": true,
    "vim.highlightedyank.enable": true,
    "vim.easymotion": true,
    "vim.easymotionMarkerFontFamily": "Inconsolata for Powerline",
    "vim.easymotionMarkerFontSize": "18",
    "vim.easymotionMarkerHeight": 18,
    "vim.easymotionMarkerWidthPerChar": 10,
    "vim.easymotionMarkerBackgroundColor": "#804002",
    "vim.easymotionMarkerForegroundColorOneChar": "#9EC285",
    "vim.easymotionMarkerForegroundColorTwoChar": "#42A5F5",
    "workbench.settings.editor": "json"
}